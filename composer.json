{
	"name": "ruslius/larahelp",
	"description": "Laravel",
	"type": "library",
	"license": "MIT",
	"authors": [
		{
			"name": "Ruslan P",
			"email": "49198@bk.ru"
		}
	],
	"require": {
		"php": ">=7",
		"barryvdh/laravel-ide-helper": "^2.10"
	},
	"require-dev": {
		"orchestra/testbench": "^6.10.0"
	},
	"autoload": {
		"psr-4": {
			"Ruslius\\Helper\\": "./src"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Ruslius\\Helper\\Tests\\": "./tests"
		}
	},
	"extra": {
		"laravel": {
			"providers": [
				"Ruslius\\Helper\\HelperServiceProvider"
			],
			"aliases": {
				"Helper": "Ruslius\\Helper\\HelperFacade"
			}
		}
	},
	"scripts": {
		"test": "./vendor/bin/phpunit"
	}
}
